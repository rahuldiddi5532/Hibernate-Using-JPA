<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
"-//Hibernate/Hibernate Mapping DTD//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
<class name="org.jsp.schoolpro.Student" table="student">
<id name="id" column="id">
<generator class="identity"/>
</id>
<property name="name" column="sname" not-null="true"/>
<property name="email" column="email" not-null="true" unique="true"/>
<property name="phone" column="sphone" not-null="true" unique="true"/>
<property name="password" column="password" not-null="true"/>

</class>
</hibernate-mapping>